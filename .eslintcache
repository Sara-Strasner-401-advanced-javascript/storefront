\n    {\n      category: 'narwhals',\n      name: 'Narwhal Onesie',\n      description: 'Won\\'t your narwhal look cute in this?!',\n      price: '$9.99',\n      inventoryCount:3\n    },\n  ],\n};\n\nexport const filterProducts = name => {\n  return {\n    type: 'FILTER',\n    payload: name\n  };\n};\n\nexport default (state = products.initialState, action) => {\n  let { type, payload } = action;\n  switch (type) {\n    case 'FILTER':\n      console.log('In the filtered function');\n      let productList = products.productList.filter((item) => item.category === payload);\n      return {productList};\n            default:\n        return state;\n  }\n};\n","/Users/sarastrasner/codefellows/401/storefront/src/components/storefront/storefront.js",[],"/Users/sarastrasner/codefellows/401/storefront/src/components/storefront/categories.js",[],"/Users/sarastrasner/codefellows/401/storefront/src/components/storefront/products.js",[],{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","severity":1,"message":"55","line":30,"column":1,"nodeType":"56","endLine":40,"endColumn":3},{"ruleId":"57","severity":1,"message":"58","line":31,"column":15,"nodeType":"59","messageId":"60","endLine":31,"endColumn":22},{"ruleId":"54","severity":1,"message":"55","line":64,"column":1,"nodeType":"56","endLine":74,"endColumn":3},"no-native-reassign",["61"],"no-negated-in-lhs",["62"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-unused-vars","'payload' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]